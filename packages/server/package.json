{
  "name": "cd-product-api",
  "version": "0.0.0",
  "description": "Server for RCA Product API",
  "main": "./dist/index.js",
  "author": "Jesse Earle",
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "jest": "^26.6.1",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.4",
    "open-cli": "^6.0.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "prestart": "yarn run:dockercompose && yarn clean && tsc",
    "start": "concurrently -c \"cyan.bold,magenta.bold,green.bold\" -n node,compile,test \"yarn node:development\" \"yarn compile:development\" \"yarn test:development\"",
    "node:development": "nodemon -r dotenv/config dist/index",
    "compile:development": "tsc -w",
    "test:development": "jest --setupFiles dotenv/config --coverage --watchAll",
    "build:production": "yarn clean && tsc -p .",
    "run:dockercompose": "docker-compose -f ./docker/docker-compose.yml up -d ",
    "clean": "rimraf ./dist",
    "pre-commit": "yarn format",
    "format": "yarn prettier --write ./src/**/*.ts"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.9.1",
    "amazon-cognito-identity-js": "4.5.4-unstable.3",
    "aws-sdk": "^2.790.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "cognito-express": "^2.0.18",
    "express": "^5.0.0-alpha.8",
    "ioredis": "^4.19.2",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.4",
    "launchdarkly-node-server-sdk": "^5.13.4",
    "morgan": "^1.10.0",
    "mssql": "^6.2.3",
    "swagger-jsdoc": "^5.0.0",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.3.0",
    "winston": "^3.3.3"
  }
}
