{
  "name": "cd-product-api",
  "version": "0.0.0",
  "description": "Server for RCA Product API",
  "main": "./dist/index.js",
  "author": "Jesse Earle",
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "start": "concurrently -c \"cyan.bold,magenta.bold\" -n node,compile \"yarn node:development\" \"yarn compile:development\"",
    "node:development": "NODE_ENV=development LOG_LEVEL=debug PORT=39000 nodemon dist/index",
    "compile:development": "tsc -w",
    "build:production": "rm -rf ./dist && tsc -p ."
  },
  "dependencies": {
    "@types/node": "^14.14.6",
    "chalk": "^4.1.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "uuid": "^8.3.0",
    "winston": "^3.3.3"
  }
}
