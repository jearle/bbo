{
  "name": "cd-product-api",
  "version": "0.0.0",
  "description": "Server for RCA Product API",
  "main": "./dist/index.js",
  "author": "Jesse Earle",
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "jest": "^26.6.1",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.4",
    "open-cli": "^6.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "prestart": "yarn clean && tsc",
    "start": "concurrently -c \"cyan.bold,magenta.bold,green.bold\" -n node,compile,test \"yarn node:development\" \"yarn compile:development\" \"yarn test:development\"",
    "node:development": "cross-env NODE_ENV=development LOG_LEVEL=debug PORT=39000 nodemon dist/index",
    "compile:development": "tsc -w",
    "test:development": "jest --coverage --watchAll",
    "build:production": "yarn clean && tsc -p .",
    "clean": "rimraf ./dist"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "mssql": "^6.2.3",
    "swagger-jsdoc": "^5.0.0",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.3.0",
    "winston": "^3.3.3"
  }
}
