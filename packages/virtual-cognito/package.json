{
  "name": "virtual-cognito",
  "version": "0.0.0",
  "description": "Virtualized Cognito service",
  "main": "./dist/index.js",
  "author": "Jesse Earle",
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/swagger-jsdoc": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.14.0",
    "eslint-watch": "^7.0.0",
    "jest": "^26.6.1",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.4",
    "open-cli": "^6.0.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.3",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "prestart": "yarn clean && tsc",
    "start": "concurrently -c \"blue.bold,cyan.bold,magenta.bold,green.bold\" -n lint,node,compile,test \"yarn lint:development\" \"yarn node:development\" \"yarn compile:development\" \"yarn test:development\"",
    "lint:development": "esw --color -w --ext .ts ./src",
    "node:development": "nodemon -r dotenv/config dist/index",
    "compile:development": "tsc -w",
    "test:development": "jest --setupFiles dotenv/config --coverage --watchAll",
    "build:production": "yarn clean && tsc -p .",
    "clean": "rimraf ./dist",
    "pre-commit": "yarn format && yarn lint",
    "format": "yarn prettier --loglevel warn --write ./src/**/*.ts",
    "lint": "yarn eslint --fix"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.9.1",
    "amazon-cognito-identity-js": "4.5.4-unstable.3",
    "aws-sdk": "^2.790.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "cognito-express": "^2.0.18",
    "express": "^5.0.0-alpha.8",
    "ioredis": "^4.19.2",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.4",
    "launchdarkly-node-server-sdk": "^5.13.4",
    "morgan": "^1.10.0",
    "mssql": "^6.2.3",
    "shared": "0.0.0",
    "swagger-jsdoc": "^5.0.0",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.3.0",
    "winston": "^3.3.3"
  }
}
